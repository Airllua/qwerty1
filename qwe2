#include <iostream>
#include <cmath>
#include <conio.h>
using namespace std;


int main()
{
	int a = 0;

	setlocale(LC_ALL, "RUS");

	cout << "Введите четырехзначное число: ";
	cin >> a;

	float a1 = a / 1000 + (a / 100) % 10;
	float a2 = (a % 100) / 10 + a % 10;

	if (a1 == a2)
	{
		cout << "Равны";
	}
	else
	{
	    cout << "Не равны";
    }

	return 0;
}
